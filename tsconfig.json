{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["ESNext"],
    "types": ["@cloudflare/vitest-pool-workers", "vitest/globals"],
    "noEmit": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["example"]
}
